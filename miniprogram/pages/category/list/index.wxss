.category-list-container {
  display: flex;
  flex-flow: column;
  height: 680px;
  margin-bottom: 40px;
  margin: 2px;
}

.category-list-container .delete {
  line-height: 50px;
  font-size: 14px;
  width: 60px;
  height: 50px;
  text-align: center;
}
.dialog-title {
  font-size: 16px;
  margin: 20px 0;
  text-align: center;
}
.dialog-input{
  border: 1px solid #f1f1f1;
  font-size: 12px;
  padding-left: 5px;
  margin: 10px;
  height: 30px;
  border-radius: 5px;
}